{
  "name": "twitchcord-v3",
  "main": "src/loader/injector.js",
  "version": "0.0.1",
  "description": "The Twitchcord client modification for Discord",
  "repository": "https://gitlab.com/Twitchcord/twitchcord-v3",
  "author": "Twitchcord",
  "private": true,
  "scripts": {
    "inject": "node src/installer/install.js inject",
    "reinject": "node src/installer/install.js reinject",
    "uninject": "node src/installer/install.js uninject",
    "node-sass-for-electron": "npm explore node-sass -- npm run build -- -m=69 --target=4.0.8",
    "watch-sass": "node resources/scripts/watch-sass.js",
    "setup": "npm install && npm run node-sass-for-electron"
  },
  "devDependencies": {
    "@types/node": "^11.10.5",
    "electron": "^7.2.4"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "btoa": "^1.2.1",
    "eventemitter3": "^4.0.0",
    "node-sass": "^4.13.0",
    "ps-node": "^0.1.6",
    "stylelint": "^12.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.13.0"
  }
}
